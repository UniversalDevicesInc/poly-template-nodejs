{
  "extends": "strongloop",
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true
  },
  "rules": {
    "indent": [
      "error",
      2, {
        "MemberExpression": 0,
        "SwitchCase": 1,
        "CallExpression": {
          "arguments": "off"
        }
      }
    ]}
}
